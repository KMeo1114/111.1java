import java.util.Scanner;

public class Test13 {
    public static void main(String[] args) {
		int num1 = 1 ;
		
		while (num1<=5){
			int num2 =1;
			while (num2 <=5-num1){
				System.out.print(" ");
				num2 +=1;
								
			}
			int row =1;
			while(row <=2*num1-1){
				System.out.print("*");
				row +=1;
			}
		    System.out.println("");
			num1 +=1;
		}
		
    }
}
